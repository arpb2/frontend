{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
      "browser": true,
      "node": true
  },
  "plugins": [
      "react-hooks"
  ],
  "rules": {
      "no-console": "warn",
      "comma-dangle": "error",
      "react/jsx-filename-extension": "warn",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
  }
}
