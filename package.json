{
  "name": "@arpb2/frontend",
  "version": "1.0.0",
  "description": "Frontend for ARPB2 project",
  "main": "src/server/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:arpb2/frontend.git"
  },
  "scripts": {
    "build": "webpack --mode production && babel -d ./dist ./src/server",
    "start": "yarn build && NODE_ENV=production node ./dist/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon --exec babel-node src/server/index.js",
    "debug": "nodemon --inspect --exec babel-node src/server/index.js",
    "start-dev": "concurrently \"yarn server\" \"yarn client\""
  },
  "author": "ARPB2",
  "license": "GPL-3.0",
  "dependencies": {
    "@material-ui/core": "^4.8.2",
    "@material-ui/icons": "^4.5.1",
    "babel-polyfill": "^6.26.0",
    "blockly": "^3.20191014.4",
    "clsx": "^1.0.4",
    "express": "^4.16.3",
    "react": "^16.5.2",
    "react-blockly": "^4.1.0",
    "react-dom": "^16.5.2",
    "recharts": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  },
  "engines": {
    "node": ">=12"
  }
}
